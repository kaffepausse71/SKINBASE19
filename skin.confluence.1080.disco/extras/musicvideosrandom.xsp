<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="musicvideos">
	<name>Random Musicvideos</name>
	<match>all</match>
	<rule field="playcount" operator="lessthan">
		<value>1</value>
	</rule>
	<order direction="ascending">random</order>
	<limit>20</limit>
</smartplaylist>
