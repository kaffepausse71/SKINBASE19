<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<smartplaylist type="albums">
	<name>Recently Added And Unplayed Albums</name>
	<match>all</match>
	<rule field="playcount" operator="lessthan">
		<value>1</value>
	</rule>
	<order direction="descending">dateadded</order>
	<limit>20</limit>
</smartplaylist>
