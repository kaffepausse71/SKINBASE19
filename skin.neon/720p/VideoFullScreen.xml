<?xml version="1.0" encoding="UTF-8"?>
<!--Alaska-->
<window>
  <controls>
    <!-- codec infos -->
    <control type="group">
      <left>4</left>
      <top>-8</top>
      <width>1280</width>
      <height>125</height>
      <visible>Control.IsVisible(10)</visible>
      <animation type="Visible">
        <effect type="fade" time="400" start="0" end="100" />
        <effect type="slide" time="400" start="0,-125" end="0,0" easing="out" tween="cubic" />
      </animation>
      <animation type="Hidden">
        <effect type="fade" time="400" start="100" end="0" />
        <effect type="slide" time="400" start="0,0" end="0,-125" easing="in" tween="cubic" />
      </animation>
      <animation type="WindowOpen">
        <effect type="fade" time="400" start="0" end="100" />
        <effect type="slide" time="400" start="0,-125" end="0,0" easing="out" tween="cubic" />
      </animation>
      <animation type="WindowClose">
        <effect type="fade" time="400" start="100" end="0" />
        <effect type="slide" time="400" start="0,0" end="0,-125" easing="in" tween="cubic" />
      </animation>
      <control type="image">
        <left>0</left>
        <top>8</top>
        <width>1280</width>
        <height>172</height>
        <texture flipx="true">common/codec_back.png</texture>
      </control>
      <control type="label" id="10">
        <description>row 1 label</description>
        <left>0</left>
        <top>12</top>
        <label>-</label>
        <align>left</align>
        <width>1280</width>
        <textcolor>white</textcolor>
        <font>small</font>
      </control>
      <control type="label" id="11">
        <description>row 2 label</description>
        <left>0</left>
        <top>37</top>
        <label>-</label>
        <align>left</align>
        <width>1280</width>
        <textcolor>white</textcolor>
        <font>small</font>
      </control>
      <control type="label" id="12">
        <description>row 3 label</description>
        <left>0</left>
        <top>62</top>
        <label>-</label>
        <align>left</align>
        <width>1280</width>
        <textcolor>white</textcolor>
        <font>small</font>
      </control>
    </control>
  </controls>
</window>
