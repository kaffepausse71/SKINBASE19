<?xml version="1.0" encoding="utf-8"?>
<includes>
    <default type="label">
        <posx>0</posx>
        <posy>0</posy>
        <height>60</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>InfoDetailsFont</font>
        <shadowcolor>ShadowColor</shadowcolor>
        <textcolor>NoFocusedColor</textcolor>
        <focusedcolor>$VAR[Picked.Theme.Color]</focusedcolor>
        <selectedcolor>SelectedColor</selectedcolor>
        <disabledcolor>DisabledColor</disabledcolor>
        <scroll>false</scroll>
        <scrollspeed>50</scrollspeed>
        <scrollsuffix>  |  </scrollsuffix>
    </default>
    <default type="fadelabel">
        <posx>0</posx>
        <posy>0</posy>
        <height>60</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>InfoDetailsFont</font>
        <textoffsetx>0</textoffsetx>
        <shadowcolor>ShadowColor</shadowcolor>
        <textcolor>NoFocusedColor</textcolor>
        <focusedcolor>$VAR[Picked.Theme.Color]</focusedcolor>
        <selectedcolor>SelectedColor</selectedcolor>
        <disabledcolor>DisabledColor</disabledcolor>
        <scrollout>false</scrollout>
        <pauseatend>3000</pauseatend>
        <scrollsuffix>  |  </scrollsuffix>
    </default>
    <default type="textbox">
        <font>InfoPlotFont</font>
        <shadowcolor>ShadowColor</shadowcolor>
        <textcolor>NoFocusedColor</textcolor>
        <focusedcolor>$VAR[Picked.Theme.Color]</focusedcolor>
        <selectedcolor>SelectedColor</selectedcolor>
        <disabledcolor>DisabledColor</disabledcolor>
        <pagecontrol>61</pagecontrol>
        <pulseonselect>false</pulseonselect>
        <include condition="Skin.HasSetting(EnableJustify)">TextboxAlignment</include>
        <align>left</align>
        <autoscroll delay="5000" time="2500" repeat="10000">!Skin.HasSetting(AutoScroll)</autoscroll>
    </default>
    <default type="image">
        <fadetime>250</fadetime>
        <align>center</align>
        <aligny>center</aligny>
        <aspectratio>stretch</aspectratio>
    </default>
    <default type="multiimage">
        <fadetime>250</fadetime>
    </default>
    <default type="button">
        <colordiffuse>$VAR[Picked.Theme.Color]</colordiffuse>
        <posx>0</posx>
        <posy>0</posy>
        <width>450</width>
        <height>90</height>
        <align>center</align>
        <aligny>center</aligny>
        <font>ButtonFont</font>
        <textoffsetx>0</textoffsetx>
        <shadowcolor>ShadowColor</shadowcolor>
        <textcolor>NoFocusedColor</textcolor>
        <focusedcolor>$VAR[Picked.Theme.Color]</focusedcolor>
        <selectedcolor>SelectedColor</selectedcolor>
        <disabledcolor>DisabledColor</disabledcolor>
        <texturefocus border="1">list/bg_facade_focus.png</texturefocus>
        <texturenofocus></texturenofocus>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="edit">
        <colordiffuse>$VAR[Picked.Theme.Color]</colordiffuse>
        <posx>0</posx>
        <posy>0</posy>
        <width>390</width>
        <height>90</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>InfoDetailsFont</font>
        <textoffsetx>20</textoffsetx>
        <shadowcolor>ShadowColor</shadowcolor>
        <textcolor>NoFocusedColor</textcolor>
        <focusedcolor>$VAR[Picked.Theme.Color]</focusedcolor>
        <selectedcolor>SelectedColor</selectedcolor>
        <disabledcolor>DisabledColor</disabledcolor>
        <texturefocus border="1">list/bg_facade_focus.png</texturefocus>
        <texturenofocus></texturenofocus>
    </default>
    <default type="togglebutton">
        <colordiffuse>$VAR[Picked.Theme.Color]</colordiffuse>
        <posx>0</posx>
        <posy>0</posy>
        <width>450</width>
        <height>90</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>ButtonFont</font>
        <textoffsetx>20</textoffsetx>
        <shadowcolor>ShadowColor</shadowcolor>
        <textcolor>NoFocusedColor</textcolor>
        <focusedcolor>$VAR[Picked.Theme.Color]</focusedcolor>
        <selectedcolor>SelectedColor</selectedcolor>
        <altselectedcolor>AltSelectedColor</altselectedcolor>
        <disabledcolor>DisabledColor</disabledcolor>
        <texturefocus border="1">list/bg_facade_focus.png</texturefocus>
        <texturenofocus></texturenofocus>
        <alttexturefocus border="1">list/bg_facade_focus.png</alttexturefocus>
        <alttexturenofocus></alttexturenofocus>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="radiobutton">
        <colordiffuse>$VAR[Picked.Theme.Color]</colordiffuse>
        <posx>0</posx>
        <posy>0</posy>
        <height>90</height>
        <align>left</align>
        <aligny>center</aligny>
        <font>ButtonFont</font>
        <shadowcolor>ShadowColor</shadowcolor>
        <textcolor>NoFocusedColor</textcolor>
        <focusedcolor>$VAR[Picked.Theme.Color]</focusedcolor>
        <selectedcolor>SelectedColor</selectedcolor>
        <disabledcolor>DisabledColor</disabledcolor>
        <radioposx>546</radioposx>
        <radiowidth>50</radiowidth>
        <radioheight>50</radioheight>
        <texturefocus border="1">list/bg_facade_focus.png</texturefocus>
        <texturenofocus></texturenofocus>
        <textureradiooffnofocus border="1">buttons/RadioOffNF.png</textureradiooffnofocus>
        <textureradioonnofocus border="1">buttons/RadioOnNF.png</textureradioonnofocus>
        <textureradioofffocus border="1">buttons/RadioOffFO.png</textureradioofffocus>
        <textureradioonfocus border="1">buttons/RadioOnFO.png</textureradioonfocus>
        <textureradioondisabled colordiffuse="90ffffff" border="1">buttons/RadioOnNF.png</textureradioondisabled>
        <textureradiooffdisabled colordiffuse="90ffffff" border="1">buttons/RadioOffNF.png</textureradiooffdisabled>
        <textoffsetx>20</textoffsetx>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="spincontrol">
        <colordiffuse>$VAR[Picked.Theme.Color]</colordiffuse>
        <posx>0</posx>
        <posy>0</posy>
        <height>90</height>
        <textureupfocus border="1">buttons/SpinUpFO.png</textureupfocus>
        <textureup border="1">buttons/SpinUpNF.png</textureup>
        <texturedownfocus border="1">buttons/SpinDownFO.png</texturedownfocus>
        <texturedown border="1">buttons/SpinDownNF.png</texturedown>
        <textureupdisabled colordiffuse="90ffffff" border="1">buttons/SpinUpNF.png</textureupdisabled>
        <texturedowndisabled colordiffuse="90ffffff" border="1">buttons/SpinDownNF.png</texturedowndisabled>
        <texturefocus border="1">list/bg_facade_focus.png</texturefocus>
        <texturenofocus></texturenofocus>
        <spinwidth>39</spinwidth>
        <spinheight>50</spinheight>
        <font>ButtonFont</font>
        <shadowcolor>ShadowColor</shadowcolor>
        <textcolor>NoFocusedColor</textcolor>
        <focusedcolor>$VAR[Picked.Theme.Color]</focusedcolor>
        <selectedcolor>SelectedColor</selectedcolor>
        <disabledcolor>DisabledColor</disabledcolor>
        <textoffsetx>20</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="spincontrolex">
        <colordiffuse>$VAR[Picked.Theme.Color]</colordiffuse>
        <posx>0</posx>
        <spinposx>40</spinposx>
        <posy>0</posy>
        <height>90</height>
        <font>ButtonFont</font>
        <textureupfocus border="1">buttons/SpinUpFO.png</textureupfocus>
        <textureup border="1">buttons/SpinUpNF.png</textureup>
        <texturedownfocus border="1">buttons/SpinDownFO.png</texturedownfocus>
        <texturedown border="1">buttons/SpinDownNF.png</texturedown>
        <textureupdisabled colordiffuse="90ffffff" border="1">buttons/SpinUpNF.png</textureupdisabled>
        <texturedowndisabled colordiffuse="90ffffff" border="1">buttons/SpinDownNF.png</texturedowndisabled>
        <texturefocus border="1">list/bg_facade_focus.png</texturefocus>
        <texturenofocus></texturenofocus>
        <spinwidth>39</spinwidth>
        <spinheight>50</spinheight>
        <shadowcolor>ShadowColor</shadowcolor>
        <textcolor>NoFocusedColor</textcolor>
        <focusedcolor>$VAR[Picked.Theme.Color]</focusedcolor>
        <selectedcolor>SelectedColor</selectedcolor>
        <disabledcolor>DisabledColor</disabledcolor>
        <textoffsetx>20</textoffsetx>
        <align>left</align>
        <aligny>center</aligny>
        <reverse>yes</reverse>
        <pulseonselect>false</pulseonselect>
    </default>
    <default type="slider">
        <colordiffuse>$VAR[Picked.Theme.Color]</colordiffuse>
        <align>left</align>
        <aligny>center</aligny>
        <visible>true</visible>
        <height>50</height>
        <sliderwidth>200</sliderwidth>
        <sliderheight>50</sliderheight>
        <texturesliderbar border="14">buttons/SliderBG.png</texturesliderbar>
        <textureslidernib>buttons/SliderNibNF.png</textureslidernib>
        <textureslidernibfocus>buttons/SliderNibFO.png</textureslidernibfocus>
        <textoffsetx>20</textoffsetx>
        <pulseonselect>false</pulseonselect>
        <orientation>horizontal</orientation>
    </default>
    <default type="sliderex">
        <colordiffuse>$VAR[Picked.Theme.Color]</colordiffuse>
        <align>left</align>
        <aligny>center</aligny>
        <height>50</height>
        <sliderwidth>200</sliderwidth>
        <sliderheight>50</sliderheight>
        <texturesliderbar border="14">buttons/SliderBG.png</texturesliderbar>
        <textureslidernib>buttons/SliderNibNF.png</textureslidernib>
        <textureslidernibfocus>buttons/SliderNibFO.png</textureslidernibfocus>
        <texturefocus border="1">list/bg_facade_focus.png</texturefocus>
        <texturenofocus></texturenofocus>
        <font>ButtonFont</font>
        <textcolor>NoFocusedColor</textcolor>
        <focusedcolor>$VAR[Picked.Theme.Color]</focusedcolor>
        <selectedcolor>SelectedColor</selectedcolor>
        <disabledcolor>DisabledColor</disabledcolor>
        <pulseonselect>false</pulseonselect>
        <textoffsetx>20</textoffsetx>
    </default>
    <default type="scrollbar">
        <colordiffuse>$VAR[Picked.Theme.Color]</colordiffuse>
        <posx>0</posx>
        <posy>0</posy>
        <width>54</width>
        <orientation>vertical</orientation>
        <!--<include>VerticalTextureSliderBackground</include>-->
        <texturesliderbarfocus border="10">buttons/Scrollbar_Vertical_Focus.png</texturesliderbarfocus>
        <texturesliderbar border="10">buttons/Scrollbar_Vertical_NoFocus.png</texturesliderbar>
        <!-- <textureslidernib border="18">buttons/Scrollbar_Vertical_NoFocus.png</textureslidernib>
        <textureslidernibfocus border="18">buttons/Scrollbar_Vertical_Focus.png</textureslidernibfocus> -->
        <pulseonselect>false</pulseonselect>
        <showonepage>false</showonepage>
    </default>
    <default type="list">
        <!-- <scrolltime>0</scrolltime> -->
        <scrolltime tween="circle" easing="out">300</scrolltime>
    </default>
    <default type="epggrid">
        <!-- <scrolltime>0</scrolltime> -->
        <scrolltime tween="circle" easing="out">300</scrolltime>
    </default>
    <default type="grouplist">
        <!-- <scrolltime>0</scrolltime> -->
        <scrolltime tween="circle" easing="out">300</scrolltime>
    </default>
    <default type="fixedlist">
        <!-- <scrolltime>0</scrolltime> -->
        <scrolltime tween="circle" easing="out">300</scrolltime>
    </default>
    <default type="panel">
        <!-- <scrolltime>0</scrolltime> -->
        <scrolltime tween="circle" easing="out">300</scrolltime>
    </default>
    <default type="wraplist">
        <!-- <scrolltime>0</scrolltime> -->
        <scrolltime tween="circle" easing="out">300</scrolltime>
    </default>
</includes>
